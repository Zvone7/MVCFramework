<configuration>
  <configSections>
    <system.webServer>
      <handlers>
        <remove name="BlockViewHandler"/>
        <add name="BlockViewHandler" path="*.js" verb="*"
            preCondition="integratedMode"
            type="System.Web.HttpNotFoundHandler" />
      </handlers>
    </system.webServer>
  </configSections>
